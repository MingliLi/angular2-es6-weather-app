{
  "port":8080,
  "apis":[
    {
      "name":"wikipedia",
      "urls":{
        "base":"https://en.wikipedia.org/w/api.php",
        "endpoints":{
          "search":"?action=query&titles=%s&prop=%s&imlimit=20&format=json",
          "getImage":"?action=query&titles=%s&format=json&prop=imageinfo&iiprop=url"
        }
      }
    },
    {
      "name":"yahoo",
      "urls":{
        "base":"https://query.yahooapis.com/v1/public/yql",
        "endpoints":{
          "weather":"?q=select%20*%20from%20weather.forecast%20where%20woeid%20in%20(select%20woeid%20from%20geo.places(1)%20where%20text%3D%22%s%22)&format=json&env=store%3A%2F%2Fdatatables.org%2Falltableswithkeys",
          "cities": "?q=select%20*%20from%20geo.places(20)%20where%20text=%22%s*%22%20AND%20placetype=%22Town%22&format=json&env=store://datatables.org/alltableswithkeys"
        }
      }
    }
  ]
}
